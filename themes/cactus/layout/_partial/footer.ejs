<footer id="footer">
  <div class="footer-left">
    <%= config.author %>
    |
    <a href="https://creativecommons.org/licenses/by-sa/4.0/"><img class="cc-by-sa"
              src="https://mirrors.creativecommons.org/presskit/buttons/80x15/png/by-sa.png" alt="by-sa"/></a>
    |
    <a href="https://www.flaticon.com/authors/freepik">Logo by Freepik</a>
  </div>
  <div class="footer-right">
    <nav>
      <ul>
        <% for (var i in theme.nav) { %> 
          <li><a href="<%- url_for(theme.nav[i]) %>"><%= __('nav.'+i).replace("nav.", "") %></a></li>
        <% } %>
      </ul>
    </nav>
  </div>
</footer>
